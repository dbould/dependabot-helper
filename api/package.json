    "test": "jest --config"
  },
  "repository": {
    "type": "git",
    "url": "dependabot-helper"
  },
  "author": "Dave Bould",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.5.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "react-bootstrap": "^1.4.3"
  },
  "devDependencies": {
    "jest": "^26.6.3"
  },
  "jest": {
